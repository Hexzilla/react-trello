{
  "name": "trello-clone",
  "description": "React components for kanbnan board",
  "main": "dist/index.js",
  "jsnext:main": "components/index.js",
  "module": "components/index.js",
  "files": [
    "src",
    "dist",
    "README"
  ],
  "scripts": {
    "lint": "eslint src/**",
    "lintfix": "eslint --fix src/**",
    "prepublish": "npm run lint && npm run build",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "storybook": "start-storybook -p 9002",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cover": "istanbul cover -x *.test.js _mocha -- -R spec --require tests/config/setup 'tests/**/*.test.js'",
    "test:report": "cat ./coverage/lcov.info | codecov && rm -rf ./coverage",
    "build": "babel src --out-dir dist --copy-files",
    "docs": "build-storybook -o docs",
    "commit": "git cz",
    "deploy-storybook": "storybook-to-ghpages"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "keywords": [
    "react",
    "trello",
    "board"
  ],
  "author": "reactomoss",
  "license": "MIT",
  "devDependencies": {
    "@storybook/addon-info": "^3.2.9",
    "@storybook/addon-options": "^3.1.2",
    "@storybook/cli": "^3.1.4",
    "@storybook/react": "^3.1.3",
    "@storybook/storybook-deployer": "^2.0.0",
    "autoprefixer": "^7.1.1",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-eslint": "^6.1.2",
    "babel-jest": "^21.0.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-es2016": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.22.0",
    "chai": "^4.0.1",
    "chai-enzyme": "^0.7.1",
    "codecov.io": "^0.1.6",
    "commitizen": "^2.9.6",
    "css-loader": "^0.28.4",
    "cz-conventional-changelog": "^2.0.0",
    "enzyme": "^2.8.2",
    "eslint": "^3.19.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-config-standard-react": "^5.0.0",
    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-node": "^5.0.0",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "^7.0.1",
    "eslint-plugin-standard": "^3.0.1",
    "eventsource-polyfill": "^0.9.6",
    "extract-text-webpack-plugin": "^2.1.0",
    "isparta": "^4.0.0",
    "istanbul": "^1.1.0-alpha.1",
    "jest": "^21.0.1",
    "jsdom": "^9.12.0",
    "mocha": "^3.4.2",
    "node-sass": "^4.5.3",
    "react-addons-test-utils": "^15.5.1",
    "react-test-renderer": "^15.4.2",
    "rimraf": "^2.6.1",
    "sass-loader": "^6.0.6",
    "semantic-release": "^6.3.6",
    "sinon": "^2.3.2",
    "style-loader": "^0.18.2",
    "webpack": "^3.0.0"
  },
  "dependencies": {
    "immutability-helper": "^2.2.3",
    "jest-cli": "^21.0.1",
    "lodash.flow": "^3.5.0",
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-dnd": "^2.2.4",
    "react-dnd-html5-backend": "^2.2.4",
    "react-dnd-multi-backend": "^2.3.7",
    "react-dnd-touch-backend": "^0.3.11",
    "react-dom": "^15.6.1",
    "react-redux": "^5.0.3",
    "redux": "^3.6.0",
    "redux-actions": "^1.2.2",
    "redux-logger": "^3.0.6",
    "styled-components": "^1.4.2"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  }
}
